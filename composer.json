{
    "name": "redeyeapps/amcharts-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2/3 Bundle that ease the use of AmCharts to display rich graph and charts in your app",
    "keywords": ["symfony2", "symfony3", "charting", "charts", "amcharts", "chart", "graph", "graphs"],
    "homepage": "https://github.com/rcousens/AmChartsBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ross Cousens"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/symfony": "~3.0"
    },
    "autoload": {
        "psr-0": {
            "RC\\AmChartsBundle": ""
        }
    },
    "target-dir": "RC/AmChartsBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
